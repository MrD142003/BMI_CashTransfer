<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CashTransfer">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="103dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="67dp"
        android:text="Nhập tiền việt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="94dp"
        android:layout_height="42dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:text="Đổi sang"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/edtMoney_Insert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerMoney_Types"
        android:layout_width="109dp"
        android:layout_height="44dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/edtMoney_Insert" />

    <Button
        android:id="@+id/btnTransfer"
        android:layout_width="120dp"
        android:layout_height="62dp"
        android:layout_marginStart="143dp"
        android:layout_marginTop="50dp"
        android:text="Transfer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerMoney_Types" />

    <TextView
        android:id="@+id/txtMoney_Result"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="51dp"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTransfer" />

    <TextView
        android:id="@+id/txtMoney_Type"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="51dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/txtMoney_Result"
        app:layout_constraintTop_toBottomOf="@+id/btnTransfer" />
</androidx.constraintlayout.widget.ConstraintLayout>